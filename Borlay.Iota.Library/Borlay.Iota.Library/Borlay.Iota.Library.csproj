<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <Authors>Paulius Kanoverskis</Authors>
    <Company>Paulius Kanoverskis</Company>
    <Description>The IOTA wallet library for .NET Standard framework. Compatible with .net 4.5 and .net core 1.0</Description>
    <RepositoryUrl>https://github.com/Borlay/Borlay.Iota.Library</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>borlay iota wallet</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/Borlay/Borlay.Iota.Library</PackageProjectUrl>
    <Version>1.0.9</Version>
    <PackageReleaseNotes>+ Add Kerl and Keccak
+ Address generation fixed
- Attach transaction still not working</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
