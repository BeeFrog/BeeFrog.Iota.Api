    <Compile Remove="Utils\ICurl.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Crypto\Sha3\" />
  </ItemGroup>

  <ItemGroup>
