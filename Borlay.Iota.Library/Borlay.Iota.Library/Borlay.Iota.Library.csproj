<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <Authors>Paulius Kanoverskis</Authors>
    <Company>Paulius Kanoverskis</Company>
    <Description>.netStandard library for IOTA wallets. You can use it for .net or .netcore projects.</Description>
    <RepositoryUrl>https://github.com/Borlay/Borlay.Iota.Library</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>borlay iota wallet</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/Borlay/Borlay.Iota.Library</PackageProjectUrl>
    <Version>1.0.4</Version>
    <PackageReleaseNotes>Changed to .netStandard 1.1 to suport .net45</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.4\Borlay.Iota.Library.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>